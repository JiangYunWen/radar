  :data=\"tableData\"\r\n                                border\r\n                                style=\"width: 100%\">\r\n                                <el-table-column prop=\"port\" label=\"进程序号\" ></el-table-column>\r\n                                <el-table-column prop=\"receive\" label=\"进程/线程名\" ></el-table-column>\r\n                                <el-table-column prop=\"receiveDiscard\" label=\"状态\"> </el-table-column>\r\n                                <el-table-column prop=\"receiveRetry\" label=\"CPU使用率\"> </el-table-column>\r\n                                <el-table-column prop=\"receiveBandwidth\" label=\"内存使用率\"> </el-table-column>\r\n                            </el-table>\r\n                        </el-card>\r\n                    </el-col>\r\n                </el-row>\r\n            </el-col> \r\n    </el-tab-pane>\r\n  </el-tabs>\r\n</div>  \r\n</template>\r\n\r\n<script>\r\nimport drawer from './drawer'\r\nexport default {\r\n    components: {\r\n    drawer\r\n  },\r\n    data(){\r\n        return{\r\n            activeName: 'first',\r\n            input:'10',\r\n            checked:false,\r\n            cpu:'',\r\n            ssd:'',\r\n            mem:'',\r\n            display: false,\r\n            drawerWidth: '500px',\r\n            tableData2: [{\r\n                port: '节点类型',\r\n                portValue:'开发板',\r\n                firmware: '固件名',\r\n                firmwareModel: 'deviceSolt ssd',\r\n                SSD:'型号',\r\n                SSDValue:'HT-04',\r\n            }, {\r\n                port: '节点状态',\r\n                portValue:'connect',\r\n                firmware: '版本号',\r\n                firmwareModel:'1.0.1',\r\n                SSD: '容量',\r\n                SSDValue:'64GB',\r\n            }\r\n            ],\r\n            tableData1: [{\r\n                cpu:'型号',\r\n                cpuValue:'HT-11 定制',\r\n                mem:'容量',\r\n                memValue:'4GB',\r\n                os:'节点编号',\r\n                osValue:'1'\r\n            }, {\r\n                cpu:'核数',\r\n                cpuValue:'11',\r\n                mem:'版本',\r\n                memValue:'0.6.2',\r\n                os:'操作系统版本',\r\n                osValue:'windows'\r\n            },{\r\n                cpu:'主频',\r\n                cpuValue:'11',\r\n                mem:'连接数',\r\n                memValue:'5',\r\n                os:'内核版本',\r\n                osValue:'麒麟'\r\n            }\r\n            ],\r\n            tableData: [{\r\n                port: '2016-05-02',\r\n                receive: '王小虎',\r\n                receiveDiscard: '上海市普陀区金沙江路 1518 弄',\r\n                receiveRetry:1,\r\n                receiveBandwidth:'disenable'\r\n            }, {\r\n                port: '2016-05-02',\r\n                receive: '王小虎',\r\n                receiveDiscard: '上海市普陀区金沙江路 1518 弄',\r\n                receiveRetry:1,\r\n                receiveBandwidth:'enable'\r\n            }, {\r\n                port: '2016-05-02',\r\n                receive: '王小虎',\r\n                receiveDiscard: '上海市普陀区金沙江路 1518 弄',\r\n                receiveRetry:1,\r\n                receiveBandwidth:'enable'\r\n            }, {\r\n                port: '2016-05-02',\r\n                receive: '王小虎',\r\n                receiveDiscard: '上海市普陀区金沙江路 1518 弄',\r\n                receiveRetry:1,\r\n                receiveBandwidth:'enable'\r\n            }]\r\n        }\r\n    },\r\n    methods:{\r\n        changeWidth () {\r\n         this.drawerWidth = (this.drawerWidth === '500px') ? '800px' : '500px'\r\n      },\r\n        drawC(){          \r\n            let myChart = this.$echarts.init(document.getElementById('container'));\r\n            myChart.setOption({\r\n              title: {\r\n                // text: '某站点用户访问来源',\r\n                // subtext: '纯属虚构',\r\n                // left: 'center'\r\n                },\r\n                tooltip: {\r\n                    trigger: 'item',\r\n                    formatter: '{a} <br/>{b} : {c} ({d}%)'\r\n                },\r\n                legend: {\r\n                    orient: 'vertical',\r\n                    left: 'left',\r\n                    data: ['已占用内存', '未占用内存']\r\n                },\r\n                series: [\r\n                    {\r\n                    name: '访问来源',\r\n                    type: 'pie',\r\n                    radius: '55%',\r\n                    center: ['50%', '50%'],\r\n                    data: [\r\n                    ],\r\n                    emphasis: {\r\n                        itemStyle: {\r\n                            shadowBlur: 10,\r\n                            shadowOffsetX: 0,\r\n                            shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n                        }\r\n                    }\r\n                    }\r\n                ]\r\n            });\r\n        },\r\n         drawC2(){\r\n            let myChart = this.$echarts.init(document.getElementById('container2'));\r\n            myChart.setOption({\r\n                    title: {\r\n                // text: '某站点用户访问来源',\r\n                // subtext: '纯属虚构',\r\n                // left: 'center'\r\n                },\r\n                tooltip: {\r\n                    trigger: 'item',\r\n                    formatter: '{a} <br/>{b} : {c} ({d}%)'\r\n                },\r\n                legend: {\r\n                    orient: 'vertical',\r\n                    left: 'left',\r\n                    data: ['已占用磁盘', '未占用磁盘']\r\n                },\r\n                series: [\r\n                    {\r\n                    name: '访问来源',\r\n                    type: 'pie',\r\n                    radius: '55%',\r\n                    center: ['35%', '50%'],\r\n                    data: [\r\n                        // {value: 310, name: '未占用磁盘'},\r\n                    ],\r\n                    emphasis: {\r\n                        itemStyle: {\r\n                            shadowBlur: 10,\r\n                            shadowOffsetX: 0,\r\n                            shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n                        }\r\n                    }\r\n                    }\r\n                ]\r\n            });\r\n        },\r\n        drawLine(){\r\n        // 基于准备好的dom，初始化echarts实例\r\n        let myChart = this.$echarts.init(document.getElementById('myChart'));\r\n        myChart.setOption({\r\n            title : {\r\n                text: 'CPU',\r\n                subtext: '',\r\n                left: 'center'\r\n            },\r\n            tooltip : {\r\n                trigger: 'axis',\r\n                axisPointer: {\r\n                 type: 'cross'\r\n             }\r\n            },\r\n            legend: {\r\n                data:['意向','预购','成交'],\r\n                bottom: 0\r\n            },\r\n            toolbox: {\r\n                show : true,\r\n                feature : {\r\n                    mark : {show: true},\r\n                    dataView : {show: true, readOnly: false},\r\n                    magicType : {show: true, type: ['line', 'bar', 'stack', 'tiled']},\r\n                    restore : {show: true},\r\n                    saveAsImage : {show: true}\r\n                }\r\n            },\r\n            calculable : true,\r\n            xAxis : [\r\n                {\r\n                    type : 'category',\r\n                    boundaryGap : false,\r\n                    data : []\r\n                }\r\n            ],\r\n            yAxis : [\r\n                {\r\n                    type : 'value',\r\n                    boundaryGap : false,\r\n                    max:100\r\n                }\r\n            ],\r\n            series : []\r\n        });\r\n      },\r\n      getCPURate(){\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/history/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){                                                     \r\n                        let myChart = this.$echarts.init(document.getElementById('myChart'));\r\n                        myChart.setOption({\r\n                              xAxis: {\r\n                                  data: res.data.data.status.times\r\n                              },\r\n                              series: [{\r\n                                  type:\"line\",\r\n                                  data:res.data.data.status.datas.memRate\r\n                              }]\r\n                          });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                });\r\n      },\r\n        getSSDRate(){\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){                                                     \r\n                        let myChart = this.$echarts.init(document.getElementById('container2'));\r\n                        myChart.setOption({\r\n                              series: [{\r\n                                  data:[{\r\n                                      value:res.data.data.status.SSDRate,\r\n                                      name: '已占用磁盘'\r\n                                  },\r\n                                  {\r\n                                      value:100-res.data.data.status.SSDRate,\r\n                                      name: '未占用磁盘'\r\n                                  }\r\n                                  ]\r\n                              }]\r\n                          });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                });\r\n      },\r\n        getMemRate(){\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){                                                     \r\n                        let myChart = this.$echarts.init(document.getElementById('container'));\r\n                        myChart.setOption({\r\n                              series: [{\r\n                                  data:[{\r\n                                      value:res.data.data.status.memRate,\r\n                                      name: '已占用内存'\r\n                                  },\r\n                                  {\r\n                                      value:100-res.data.data.status.memRate,\r\n                                      name: '未占用内存'\r\n                                  }\r\n                                  ]\r\n                              }]\r\n                          });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                });\r\n      },\r\n      checkedFun(e,checkedFresh){\r\n            if(e==true){\r\n                 this.cpu=setInterval(this.getCPURate,(parseInt(this.input) *1000));\r\n                 this.ssd=setInterval(this.getSSDRate,(parseInt(this.input) *1000));\r\n                 this.mem=setInterval(this.getMemRate,(parseInt(this.input) *1000));\r\n            }else{\r\n                clearInterval(this.cpu);\r\n                clearInterval(this.ssd);\r\n                clearInterval(this.mem);\r\n            }\r\n      }\r\n\r\n    },\r\n    mounted:function(){\r\n        this.drawC();\r\n        this.drawC2();\r\n        this.drawLine();\r\n        this.getCPURate();\r\n        this.getSSDRate();\r\n        this.getMemRate();\r\n    }\r\n}\r\n</script>\r\n<style lang=\"scss\">\r\n.btn { \r\n  border: none;\r\n  border-radius: 3px;\r\n  outline: none;\r\n  cursor: pointer;\r\n  letter-spacing: 3px;\r\n  &:hover {\r\n    opacity: .8;\r\n  }\r\n  &.open {\r\n    width: 200px;\r\n    height: 80px;\r\n    font-size: 18px;\r\n    background-color: orange;\r\n    color: #fff;\r\n  }\r\n  &.try {\r\n    width: 100px;\r\n    height: 40px;\r\n    background-color: green;\r\n    color: #fff;\r\n    margin-top: 50px;\r\n  }\r\n}\r\n</style>"]}]}