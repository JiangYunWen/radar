  </el-table>\r\n                        </el-card>\r\n                    </el-col>\r\n                </el-row>\r\n                <el-row :gutter=\"20\" class=\"mgb20\">\r\n                    <el-col :span=\"12\">\r\n                        <el-card shadow=\"hover\" :body-style=\"{padding: '0px'}\" style=\"height:400px; width:1500px;\">\r\n                           <div slot=\"header\" class=\"clearfix\">\r\n                            <h3>网卡信息</h3>\r\n                       </div>    \r\n                            <el-table\r\n                                :data=\"tableData\"\r\n                                border\r\n                                style=\"width: 100%\">\r\n                                <el-table-column prop=\"port\" label=\"网卡名称\" ></el-table-column>\r\n                                <el-table-column prop=\"receive\" label=\"网卡类型\" ></el-table-column>\r\n                                <el-table-column prop=\"receiveDiscard\" label=\"IP地址\"> </el-table-column>\r\n                                <el-table-column prop=\"receiveRetry\" label=\"MAC地址\"> </el-table-column>\r\n                                <el-table-column prop=\"receiveBandwidth\" label=\"网卡带宽\"> </el-table-column>\r\n                                <el-table-column prop=\"contract\" label=\"状态\"> </el-table-column>\r\n                                <el-table-column prop=\"connectionStatus\" label=\"使能\"> \r\n                                <template slot-scope=\"scope\">\r\n                                    <span v-if=\"scope.row.connectionStatus==='up'\" style=\"color:green;\">up</span>\r\n                                    <span v-if=\"scope.row.connectionStatus==='down'\" style=\"color:red;\">down</span>                     \r\n                               </template>\r\n                                </el-table-column>\r\n                            </el-table>\r\n                        <div style=\"display:flex;margin:60px 600px;\">\r\n                            <el-button type=\"primary\" style=\"width:150px;margin-right:30px;\" >获取</el-button>\r\n                            <el-button type=\"primary\" style=\"width:150px;\">设置</el-button>\r\n                        </div>\r\n                        </el-card>\r\n                    </el-col>\r\n                </el-row>\r\n                  <el-row :gutter=\"20\" class=\"mgb20\">\r\n                    <el-col :span=\"12\">\r\n                        <el-card shadow=\"hover\" :body-style=\"{padding: '0px'}\" style=\"width:1500px;\">\r\n                           <div slot=\"header\" class=\"clearfix\">\r\n                            <h3>系统进程运行状态</h3>\r\n                       </div>    \r\n                            <el-table\r\n                                :data=\"tableData\"\r\n                                border\r\n                                style=\"width: 100%\">\r\n                                <el-table-column prop=\"port\" label=\"进程序号\" ></el-table-column>\r\n                                <el-table-column prop=\"receive\" label=\"进程/线程名\" ></el-table-column>\r\n                                <el-table-column prop=\"receiveDiscard\" label=\"状态\"> </el-table-column>\r\n                                <el-table-column prop=\"receiveRetry\" label=\"CPU使用率\"> </el-table-column>\r\n                                <el-table-column prop=\"receiveBandwidth\" label=\"内存使用率\"> </el-table-column>\r\n                            </el-table>\r\n                        </el-card>\r\n                    </el-col>\r\n                </el-row>\r\n            </el-col> \r\n    </el-tab-pane>\r\n  </el-tabs>\r\n</div>  \r\n</template>\r\n\r\n<script>\r\nimport RightPanel from '@/components/RightPanel'\r\nimport Settings  from '@/components/Settings'\r\nexport default {\r\n      components: {\r\n           RightPanel,\r\n            Settings\r\n      },\r\n    data(){\r\n        return{\r\n            activeName: 'first',\r\n            input:1,\r\n            checked:false,\r\n            cpu:'',\r\n            ssd:'',\r\n            mem:'',\r\n            rx:'',\r\n            tx:'',\r\n            series_data:new Array(),\r\n            legend_names:new Array(),\r\n            rx_name:new Array(),\r\n            rx_data:new Array(),\r\n            tx_name:new Array(),\r\n            tx_data:new Array(),\r\n            drawer: false,\r\n            drawerWidth: '500px',\r\n            tableData2: [{\r\n                port: '节点类型',\r\n                portValue:'开发板',\r\n                firmware: '固件名',\r\n                firmwareModel: 'deviceSolt ssd',\r\n                SSD:'型号',\r\n                SSDValue:'HT-04',\r\n            }, {\r\n                port: '节点状态',\r\n                portValue:'connect',\r\n                firmware: '版本号',\r\n                firmwareModel:'1.0.1',\r\n                SSD: '容量',\r\n                SSDValue:'64GB',\r\n            }\r\n            ],\r\n            tableData1: [{\r\n                cpu:'型号',\r\n                cpuValue:'HT-11 定制',\r\n                mem:'容量',\r\n                memValue:'4GB',\r\n                os:'节点编号',\r\n                osValue:'1'\r\n            }, {\r\n                cpu:'核数',\r\n                cpuValue:'11',\r\n                mem:'版本',\r\n                memValue:'0.6.2',\r\n                os:'操作系统版本',\r\n                osValue:'windows'\r\n            },{\r\n                cpu:'主频',\r\n                cpuValue:'11',\r\n                mem:'连接数',\r\n                memValue:'5',\r\n                os:'内核版本',\r\n                osValue:'麒麟'\r\n            }\r\n            ],\r\n            tableData: [{\r\n                port: '2016-05-02',\r\n                receive: '王小虎',\r\n                receiveDiscard: '上海市普陀区金沙江路 1518 弄',\r\n                receiveRetry:1,\r\n                receiveBandwidth:'disenable'\r\n            }, {\r\n                port: '2016-05-02',\r\n                receive: '王小虎',\r\n                receiveDiscard: '上海市普陀区金沙江路 1518 弄',\r\n                receiveRetry:1,\r\n                receiveBandwidth:'enable'\r\n            }, {\r\n                port: '2016-05-02',\r\n                receive: '王小虎',\r\n                receiveDiscard: '上海市普陀区金沙江路 1518 弄',\r\n                receiveRetry:1,\r\n                receiveBandwidth:'enable'\r\n            }, {\r\n                port: '2016-05-02',\r\n                receive: '王小虎',\r\n                receiveDiscard: '上海市普陀区金沙江路 1518 弄',\r\n                receiveRetry:1,\r\n                receiveBandwidth:'enable'\r\n            }]\r\n        }\r\n    },\r\n    methods:{\r\n        getrealtime(){\r\n          this.legend_names=[];\r\n          this.series_data=[];\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/realtime/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){   \r\n\r\n                        for(var key in res.data.cpu.series){\r\n                            this.legend_names.push(key);\r\n                            var swpdata={};\r\n                            swpdata['name']=key;\r\n                            swpdata['type']=\"line\";\r\n                            swpdata['data']=res.data.cpu.series[key];\r\n                            this.series_data.push(swpdata);\r\n                       \r\n                    }\r\n                        // this.legend_names.shift();\r\n                        // this.series_data.shift();\r\n                        // res.data.cpu.times.shift();\r\n                        console.log(this.legend_names);\r\n                        console.log(this.series_data);\r\n                        let myChart = this.$echarts.init(document.getElementById('myChart'));\r\n                          myChart.setOption({\r\n                                legend: {\r\n                                    data:this.legend_names,\r\n                                    bottom: 0\r\n                                },\r\n                                xAxis: {\r\n                                    data: res.data.cpu.date\r\n                                },\r\n                                series:this.series_data\r\n                            });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                    console.log(error);\r\n                });\r\n      },\r\n      getCPURate(){\r\n          this.legend_names=[];\r\n          this.series_data=[];\r\n            this.rx_name=[];\r\n          this.rx_data=[];\r\n            this.tx_name=[];\r\n          this.tx_data=[];\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/history/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){   \r\n\r\n                        for(var key in res.data.cpu.series){\r\n                            this.legend_names.push(key);\r\n                            var swpdata={};\r\n                            swpdata['name']=key;\r\n                            swpdata['type']=\"line\";\r\n                            swpdata['data']=res.data.cpu.series[key];\r\n                            this.series_data.push(swpdata);\r\n                       \r\n                    }\r\n                        this.legend_names.shift();\r\n                        this.series_data.shift();\r\n                        res.data.cpu.times.shift();\r\n                        let myChart = this.$echarts.init(document.getElementById('myChart'));\r\n                          myChart.setOption({\r\n                                legend: {\r\n                                    data:this.legend_names,\r\n                                    bottom: 0\r\n                                },\r\n                                xAxis: {\r\n                                    data: res.data.cpu.times\r\n                                },\r\n                                series:this.series_data\r\n                            });\r\n\r\n                    for(var key in res.data.netrx.series){\r\n                            this.rx_name.push(key);\r\n                            var swpdata={};\r\n                            swpdata['name']=key;\r\n                            swpdata['type']=\"line\";\r\n                            swpdata['data']=res.data.netrx.series[key];\r\n                            this.rx_data.push(swpdata);\r\n                       \r\n                    }\r\n                    let Chartnetout = this.$echarts.init(document.getElementById('Chartnetout'));\r\n                          Chartnetout.setOption({\r\n                                legend: {\r\n                                    data:this.rx_name,\r\n                                    bottom: 0\r\n                                },\r\n                                xAxis: {\r\n                                    data: res.data.netrx.date\r\n                                },\r\n                                series:this.rx_data\r\n                            });\r\n\r\n\r\n                    for(var key in res.data.nettx.series){\r\n                            this.tx_name.push(key);\r\n                            var swpdata={};\r\n                            swpdata['name']=key;\r\n                            swpdata['type']=\"line\";\r\n                            swpdata['data']=res.data.nettx.series[key];\r\n                            this.tx_data.push(swpdata);\r\n                       \r\n                    }\r\n                        // this.legend_names.shift();\r\n                        // this.series_data.shift();\r\n                        // res.data.cpu.times.shift();\r\n                        console.log(this.tx_name);\r\n                        console.log(this.tx_data);\r\n                        let Chartnetin = this.$echarts.init(document.getElementById('Chartnetin'));\r\n                          Chartnetin.setOption({\r\n                                legend: {\r\n                                    data:this.tx_name,\r\n                                    bottom: 0\r\n                                },\r\n                                xAxis: {\r\n                                    data: res.data.nettx.date\r\n                                },\r\n                                series:this.tx_data\r\n                            });\r\n                    \r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                         clearInterval(this.cpu);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                    clearInterval(this.cpu);\r\n                });\r\n      },\r\n        getnetrx(){\r\n          this.rx_name=[];\r\n          this.rx_data=[];\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/history/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){   \r\n\r\n                        for(var key in res.data.netrx.series){\r\n                            this.rx_name.push(key);\r\n                            var swpdata={};\r\n                            swpdata['name']=key;\r\n                            swpdata['type']=\"line\";\r\n                            swpdata['data']=res.data.netrx.series[key];\r\n                            this.rx_data.push(swpdata);\r\n                       \r\n                    }\r\n                        // this.legend_names.shift();\r\n                        // this.series_data.shift();\r\n                        // res.data.cpu.times.shift();\r\n                        console.log(this.rx_name);\r\n                        console.log(this.rx_data);\r\n                        let myChart = this.$echarts.init(document.getElementById('Chartnetout'));\r\n                          myChart.setOption({\r\n                                legend: {\r\n                                    data:this.rx_name,\r\n                                    bottom: 0\r\n                                },\r\n                                xAxis: {\r\n                                    data: res.data.netrx.date\r\n                                },\r\n                                series:this.rx_data\r\n                            });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                    console.log(error);\r\n                });\r\n      },\r\n        getnettx(){\r\n          this.tx_name=[];\r\n          this.tx_data=[];\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/history/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){   \r\n\r\n                        for(var key in res.data.nettx.series){\r\n                            this.tx_name.push(key);\r\n                            var swpdata={};\r\n                            swpdata['name']=key;\r\n                            swpdata['type']=\"line\";\r\n                            swpdata['data']=res.data.nettx.series[key];\r\n                            this.tx_data.push(swpdata);\r\n                       \r\n                    }\r\n                        // this.legend_names.shift();\r\n                        // this.series_data.shift();\r\n                        // res.data.cpu.times.shift();\r\n                        console.log(this.tx_name);\r\n                        console.log(this.tx_data);\r\n                        let myChart = this.$echarts.init(document.getElementById('Chartnetin'));\r\n                          myChart.setOption({\r\n                                legend: {\r\n                                    data:this.tx_name,\r\n                                    bottom: 0\r\n                                },\r\n                                xAxis: {\r\n                                    data: res.data.nettx.date\r\n                                },\r\n                                series:this.tx_data\r\n                            });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                    console.log(error);\r\n                });\r\n      },\r\n        getSSDRate(){\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){                                                     \r\n                        let myChart = this.$echarts.init(document.getElementById('container2'));\r\n                        myChart.setOption({\r\n                              series: [{\r\n                                  data:[{\r\n                                      value:res.data.data.status.SSDRate,\r\n                                      name: '已占用磁盘'\r\n                                  },\r\n                                  {\r\n                                      value:100-res.data.data.status.SSDRate,\r\n                                      name: '未占用磁盘'\r\n                                  }\r\n                                  ]\r\n                              }]\r\n                          });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                });\r\n      },\r\n        getMemRate(){\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){                                                     \r\n                        let myChart = this.$echarts.init(document.getElementById('container'));\r\n                        myChart.setOption({\r\n                              series: [{\r\n                                  data:[{\r\n                                      value:res.data.data.status.memRate,\r\n                                      name: '已占用内存'\r\n                                  },\r\n                                  {\r\n                                      value:100-res.data.data.status.memRate,\r\n                                      name: '未占用内存'\r\n                                  }\r\n                                  ]\r\n                              }]\r\n                          });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                });\r\n      },\r\n      checkedFun(e,checkedFresh){\r\n          console.log(e);\r\n            if(e==true){\r\n                 this.cpu=setInterval(this.getCPURate,(parseInt(this.input) *500));\r\n                //  this.ssd=setInterval(this.getSSDRate,(parseInt(this.input) *1000));\r\n                //  this.mem=setInterval(this.getMemRate,(parseInt(this.input) *1000));\r\n                //  this.rx=setInterval(this.getnetrx,(parseInt(this.input) *1000));\r\n                //  this.tx=setInterval(this.getnettx,(parseInt(this.input) *1000));\r\n            }else if(e==false){\r\n                clearInterval(this.cpu);\r\n                // clearInterval(this.ssd);\r\n                // clearInterval(this.mem);\r\n                // clearInterval(this.rx);\r\n                // clearInterval(this.tx);\r\n            }\r\n      }\r\n\r\n    },\r\n    mounted:function(){\r\n        // this.cpu=setInterval(this.getCPURate,1000);\r\n        this.getCPURate();\r\n        this.getnetrx();\r\n        this.getnettx();\r\n        this.getSSDRate();\r\n        this.getMemRate();\r\n    }\r\n}\r\n</script>"]}]}