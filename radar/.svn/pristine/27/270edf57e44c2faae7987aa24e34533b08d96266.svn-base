 line-height: 60px; */\n    width: 1100px;\n    height: 90px;\n  }\n  \n\n  .box{\n  /* width: 1000px;\n     height: 400px; */\n     display: flex; ;\n     background-color: #E9EEF3;\n\t/* border:1px solid black; */\n    /* flex-direction: column; */\n  }\n  .el-main {\n      display: flex; \n      background-color: #E9EEF3;\n      color: #333;\n      text-align: center;\n      line-height: 140px;\n      width: 1100px;\n      height: 435px;\n    }\n  \n  body > .el-container {\n    margin-bottom: 20px;\n  }\n\n  .cont{\n      position: relative;\n      width:660px;\n      height: 250px;\n      border:1px solid black;\n      margin: 15px 2px;\n      display: flex;\n  }\n\n.contsolt{\n      position: relative;\n      width:45px; \n      height:200px;\n      background-color: #e7e7e7; \n      border:1px solid black;\n      margin: 10px 2px;\n}\n\n  /* 复选框 */\n.checkbox{\n    position: absolute;\n    bottom:-30px;\n    left: 10px;\n}\n  /* 状态 */\n.status{\n    position: absolute;\n    bottom:170px;\n    font-size: 12px;\n    left: 10px;\n    color:blue;\n}\n  /* 状态显示 */\n  .image {\n     position: absolute; \n     width: 20px;  \n     bottom:150px;\n     left: 10px;\n}\n  /* cpu数*/\n  .cpu{\n    position: absolute;\n    font-size: 12px;\n    bottom:130px;\n    left: 10px;\n  }\n  /* cpu数值 */\n  .bat{\n    position: absolute;\n    bottom:110px;\n    left: 10px;\n    width: 18px;\n    height: 18px;\n  }\n  /* 内存 */\n  .memory{\n    position: absolute;\n    font-size: 12px;\n    bottom:90px;\n    left: 10px;\n  }\n  /* 内存数值 */\n  .memorybat{\n    position: absolute;\n    bottom:70px;\n    left: 10px;\n  }\n /* SSD */\n  .ssd{\n    position: absolute;\n    font-size: 12px;\n    bottom:50px;\n    left: 10px;\n  }\n  /* 内存数值 */\n  .ssdbat{\n    position: absolute;\n    bottom:30px;\n    left: 10px;\n  }\n\n  /* 刀片名称*/\n  .name{\n    position: absolute;\n    font-size: 12px;\n    bottom:0px;\n    left: 5px;\n    transform: scale(0.75);\n    white-space: nowrap;  /*强制span不换行*/\n    display: inline-block;  /*将span当做块级元素对待*/\n    width: 25px;  /*限制宽度*/\n  }\n\n.center{\n    position: relative;\n    left: 80px;\n    \n}\n\n.center div{\n    position: relative;\n    display: inline-block;\n    margin: 5px 10px;\n}\n\n.moring{\n    position: absolute;\n    bottom:-30px;\n    left: 10px;\n}\n  \n",{"version":3,"sources":["Donate.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmaA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"Donate.vue","sourceRoot":"src/components/page","sourcesContent":["<template>\n<div class=\"container\">\n  <el-tabs v-model=\"activeName\" type=\"card\" @tab-click=\"handleClick\">\n    <el-tab-pane label=\"配置\" name=\"first\">\n        <el-container>                        \n              <div >              \n                <el-row :gutter=\"20\" >\n                 <el-col :span=\"12\"  v-for=\"device in items\" :key=\"device.id\">\n                   <el-card shadow=\"hover\" class=\"mgb20\" >\n                      <div slot=\"header\" class=\"clearfix\">\n                            <span>{{device.name}}</span>\n                      </div>                    \n                      <Test :msg=\"device.sublist\"></Test>                                  \n                   </el-card>               \n                </el-col>           \n                </el-row>               \n              </div>\n        </el-container>\n    </el-tab-pane>\n    <el-tab-pane label=\"状态\" name=\"second\" >\n                <el-col :span=\"16\">\n                <el-row :gutter=\"20\" class=\"mgb20\">\n                    <el-col :span=\"12\">\n                        <el-card shadow=\"hover\" :body-style=\"{padding: '0px'}\" style=\"height:400px;width:1000px;\">\n                        <div slot=\"header\" class=\"clearfix\">\n                            <span>风扇转速</span>\n                       </div>    \n                            <div id=\"myChart\" :style=\"{width: '1000px', height: '350px'}\"></div>\n                        </el-card>\n                    </el-col>\n                </el-row>\n                <el-row :gutter=\"20\" class=\"mgb20\">\n                    <el-col :span=\"12\">\n                        <el-card shadow=\"hover\" :body-style=\"{padding: '0px'}\" style=\"height:400px;width:1000px;\">\n                        <div slot=\"header\" class=\"clearfix\">\n                            <span>机箱温度</span>\n                       </div>    \n                            <div id=\"myChart2\" :style=\"{width: '1000px', height: '350px'}\"></div>\n                        </el-card>\n                    </el-col>\n                </el-row>\n            </el-col>      \n    </el-tab-pane>\n    <el-tab-pane label=\"报警\" name=\"third\">\n                <div class=\"filter-container\">\n                 <div class=\"letf-items\" style=\"float: left; \">\n                 <el-button\n                    @click=\"handleUpdatetrue\"\n                ><i class=\"el-icon-success\" style=\"font-size:12px;color:#00EC00;margin-right: 5px;\" ></i>确认</el-button>\n                 <el-button\n                    @click=\"handleUpdatefalse\"\n                ><i class=\"el-icon-remove\" style=\"font-size:12px;color:#FF2D2D;margin-right: 5px;\" ></i>取消确认</el-button>\n                  <el-button\n                    @click=\"handleclearflagtrue\"\n                ><i class=\"el-icon-error\" style=\"font-size:12px;color:#FF2D2D;margin-right: 5px;\" ></i>清除</el-button>\n                  <el-button\n                    @click=\"exportExcel\"\n                ><i class=\"el-icon-upload\" style=\"font-size:12px;color:#0080FF;margin-right: 5px;\" ></i>导出</el-button>\n                <!-- <el-select v-model=\"query.address\" placeholder=\"地址\" class=\"handle-select mr10\">\n                    <el-option key=\"1\" label=\"广东省\" value=\"广东省\"></el-option>\n                    <el-option key=\"2\" label=\"湖南省\" value=\"湖南省\"></el-option>\n                </el-select> -->\n                </div>\n                <div class=\"right-items\" style=\"float: right;\">\n                <span class=\"\">级别：</span>\n               <el-select v-model=\"query.grade\" placeholder=\"全部\" class=\"handle-select \">\n                    <el-option key=\"0\" label=\"全部\" value=\"\"></el-option>\n                    <el-option key=\"1\" label=\"紧急\" value=\"0\"></el-option>\n                    <el-option key=\"2\" label=\"重要\" value=\"1\"></el-option>\n                    <el-option key=\"3\" label=\"次要\" value=\"2\"></el-option>\n                    <el-option key=\"4\" label=\"提示\" value=\"3\"></el-option>\n                </el-select> \n                &nbsp;\n                <el-button  @click=\"handleSearch\">\n                <i class=\"el-icon-search\" style=\"font-size:12px;color:#0080FF;margin-right: 5px;\" ></i>\n                查询</el-button>\n                &nbsp;\n                <el-select v-model=\"query.address\" placeholder=\"高级查询\" class=\"handle-select \">\n                    <el-option key=\"1\" label=\"广东省\" value=\"广东省\"></el-option>\n                    <el-option key=\"2\" label=\"湖南省\" value=\"湖南省\"></el-option>\n                </el-select>\n                </div>\n            </div>\n\n            <el-table\n                :data=\"tableData1\"\n                border\n                class=\"table\"\n                ref=\"multipleTable\"\n                header-cell-class-name=\"table-header\"\n                @selection-change=\"handleSelectionChange\"\n            >\n                <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\n                 <!--el-table-column prop=\"id\" label=\"ID\" width=\"55\" align=\"center\"></el-table-column-->\n                <el-table-column prop=\"grade\" label=\"级别\" >\n                  <template slot-scope=\"scope\">\n                        <i v-if=\"scope.row.grade===0\" class=\"el-icon-error\" style=\"color:#FF2D2D\"></i>\n                        <i v-if=\"scope.row.grade===1\" class=\"el-icon-warning\" style=\"color:#FF2D2D\"></i>\n                        <i v-if=\"scope.row.grade===2\" class=\"el-icon-s-opportunity\" style=\"color:#FFD306\"></i>\n                        <i v-if=\"scope.row.grade===3\" class=\"el-icon-question\" style=\"color:#0080FF\"></i>\n                        <span v-if=\"scope.row.grade===0\" >紧急</span>\n                        <span v-if=\"scope.row.grade===1\" >重要</span>\n                        <span v-if=\"scope.row.grade===2\" >次要</span>\n                        <span v-if=\"scope.row.grade===3\" >提示</span>\n                        \n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"ackFlag\" label=\"确认状态\">\n                       <template slot-scope=\"scope\">\n                        <span v-if=\"scope.row.ackFlag===false\" style=\"color:#FF2D2D\">未确认</span>\n                        <span v-if=\"scope.row.ackFlag===true\" style=\"color:#00EC00\">已确认</span>                       \n                    </template>\n                </el-table-column>\n\n                <el-table-column prop=\"clearFlag\" label=\"清除状态\" >\n                   <template slot-scope=\"scope\">\n                        <span v-if=\"scope.row.clearFlag===false\" style=\"color:#FF2D2D\">未清除</span>\n                        <span v-if=\"scope.row.clearFlag===true\" style=\"color:#00EC00\">已清除</span>                       \n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"name\" label=\"名称\" >\n                    <template slot-scope=\"scope\">\n                        <span  style=\"color:#0080FF\">{{scope.row.name}}</span>                      \n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"number\" label=\"次数\" >\n                      <template slot-scope=\"scope\">\n                        <span  style=\"color:#0080FF\">{{scope.row.number}}</span>                      \n                    </template>\n                </el-table-column>\n                <el-table-column prop=\"type\" label=\"类型\" ></el-table-column>\n                <el-table-column prop=\"source\" label=\"来源\" width=\"250\"></el-table-column>\n                <el-table-column prop=\"createTime\" label=\"第一次产生时间\"  width=\"200\"></el-table-column>\n                <el-table-column prop=\"editTime\" label=\"最后一次产生时间\"  width=\"200\">\n                    <!-- <template slot-scope=\"scope\">\n                        <el-tag\n                            :type=\"scope.row.state==='成功'?'success':(scope.row.state==='失败'?'danger':'')\"\n                        >{{scope.row.state}}</el-tag>\n                    </template> -->\n                </el-table-column>\n\n                <!--el-table-column label=\"操作\" width=\"180\" align=\"center\">\n                    <template slot-scope=\"scope\">\n                        <el-button\n                            type=\"text\"\n                            icon=\"el-icon-edit\"\n                            @click=\"handleEdit(scope.$index, scope.row)\"\n                        >编辑</el-button> \n                        <el-button\n                            type=\"text\"\n                            icon=\"el-icon-delete\"\n                            class=\"red\"\n                            @click=\"handleDelete(scope.$index, scope.row)\"\n                        >删除</el-button>\n                    </template>\n                </el-table-column-->\n            </el-table>\n            <div class=\"pagination\">\n                  <el-pagination \n                    @size-change=\"changeSizeHandle\" \n                    @current-change=\"currentChangeHandle\"\n                    :current-page=\"query.pageIndex\"  \n                    layout=\"total, sizes, prev, pager, next, jumper\" \n                    :total=\"pageTotal\" background>\n                </el-pagination>\n\n            </div>\n    </el-tab-pane>\n  </el-tabs>\n</div>\n\n</template>\n\n<script>\n// import ss from \"../../../node_modules/gojs/extensions/Figures\"\n  // import ss from \"../../../node_modules/jquery/dist/jquery\"\n//  let $ = go.GraphObject.make;  // 后面很多用到该变量来初始化diagram\nimport 'jquery-ui/ui/widgets/draggable'\nimport draw2d from 'draw2d'\nimport { fetchData } from '../../api/index';\n\nconst cityOptions = ['上海', '北京', '广州', '深圳'];\nexport default {\n    draw2d:{ draw2d },\n    data() {\n     return {\n        activeName: 'first',\n        checkAll: false,\n        checkedCities: ['上海', '北京'],\n        cities: cityOptions,\n        isIndeterminate: true,\n        items:[],\n        query: {\n                address: '',\n                name: '',\n                 grade: '',\n                limit:10,\n                pageIndex: 1,\n                pageSize: 10\n            },\n        tableData1: [],\n        multipleSelection: [],\n        delList: [],\n        pageTotal: 0,\n      };\n  },\n    methods: {    \n        // 获取 报警的模拟数据\n        getData(str) {\n                this.$axios({\n                    method:\"post\",\n                    url:'/sys/warn/listData',\n                    data:JSON.stringify({page:this.query.pageIndex,limit:this.query.pageSize,grade:this.query.grade}),\n                    headers: {\n                     'Content-Type': \"application/json\"\n                   },\n                    withCredentials:true,\n                }).then(res => {\n                    if(res.data.code==0){                                                     \n                        this.tableData1 = res.data.page.list;\n                        this.pageTotal = res.data.page.totalCount || 10;//1页数据\n                        this.query.pageIndex=res.data.page.currPage;//当前页\n                        this.query.pageSize=res.data.page.pageSize;//数据总数\n                    }else{\n                         this.$message.error(res.data.msg);\n                    }\n                }).catch(error => {\n                    this.$message.error('获取数据失败！');\n                });\n        }, \n      //机箱数据获取\n      getDevice(){\n                this.$axios({\n                    method:\"get\",\n                    url:'/sys/device/status/tree?devicePid=246',\n                    withCredentials:true,\n                }).then(res => {\n                    if(res.data.code==0){                                                     \n                        this.items=res.data.devices;\n                    }else{\n                         this.$message.error(res.data.msg);\n                    }\n                }).catch(error => {\n                    this.$message.error('获取数据失败！');\n                });\n      },\n      exportExcel () {\n            /* generate workbook object from table */\n            let wb = XLSX.utils.table_to_book(document.querySelector('.table'));\n            /* get binary string as output */\n            let wbout = XLSX.write(wb, { bookType: 'xlsx', bookSST: true, type: 'array' });\n            try {\n                FileSaver.saveAs(new Blob([wbout], { type: 'application/octet-stream' }), '系统日志.xlsx');\n            } catch (e)\n            {\n                if (typeof console !== 'undefined')\n                    console.log(e, wbout)\n            }\n            return wbout\n        },\n        // 触发搜索按钮\n        handleSearch() {\n            this.getData();\n        },\n        // 取消确认操作\n        handleUpdatefalse() {\n            const length = this.multipleSelection.length;\n            let str = [];\n            this.delList = this.delList.concat(this.multipleSelection);\n            //同时确认多个\n            for (let i = 0; i < this.delList.length; i++) {\n                str.push(this.delList[i].id+\"\");               \n            }\n\n            this.$confirm('是否将确认状态修改为未确认？', '提示', {\n                type: 'warning'\n            }).then(() => {\n                    this.ackflagUpdatefalse(str);\n                     this.multipleSelection = [];\n                })\n                .catch(() => {});\n        },\n        ackflagUpdatefalse(str){   \n            this.$axios({\n                method: \"POST\",\n                url: '/sys/warn/ackflag/false',\n                data: JSON.stringify(str),\n                // 设置请求头\n               headers: {\n                    'Content-Type': \"application/json\"\n                },\n                 withCredentials:true,//跨域请求\n            }).then((res) => {\n                let data = res.data\n                if(data.code==0) {         \n                    this.$message.success('操作完成') ;\n                    this.getData();//局部刷新表格               \n                } else {\n                    this.$message.error(data.msg || '操作失败')\n                }\n            }).catch((e)=>{\n                this.$message.error(e)\n            });\n        },\n        // 已确认操作\n        handleUpdatetrue() {\n            const length = this.multipleSelection.length;\n            let str = [];\n            this.delList = this.delList.concat(this.multipleSelection);\n            //同时确认多个\n            for (let i = 0; i < this.delList.length; i++) {\n                str.push(this.delList[i].id+\"\");               \n            }\n\n            this.$confirm('是否将确认状态修改为已确认？', '提示', {\n                type: 'warning'\n            }).then(() => {\n                    this.ackflagUpdatetrue(str);\n                     this.multipleSelection = [];\n                    // this.$message.success('删除成功');\n                    // this.tableData.splice(index, 1);\n                })\n                .catch(() => {});\n        },\n        ackflagUpdatetrue(str){   \n            this.$axios({\n                method: \"POST\",\n                url: '/sys/warn/ackflag/true',\n                data: JSON.stringify(str),\n                // 设置请求头\n               headers: {\n                    'Content-Type': \"application/json\"\n                },\n                 withCredentials:true,//跨域请求\n            }).then((res) => {\n                let data = res.data\n                if(data.code==0) {         \n                    this.$message.success('操作完成') ;\n                    this.getData();//局部刷新表格               \n                } else {\n                    this.$message.error(data.msg || '操作失败')\n                }\n            }).catch((e)=>{\n                this.$message.error(e)\n            });\n        },\n        // 已清除操作\n        handleclearflagtrue() {\n            const length = this.multipleSelection.length;\n            let str = [];\n            this.delList = this.delList.concat(this.multipleSelection);\n            //同时确认多个\n            for (let i = 0; i < this.delList.length; i++) {\n                str.push(this.delList[i].id+\"\");               \n            }\n\n            this.$confirm('是否清除数据？', '提示', {\n                type: 'warning'\n            }).then(() => {\n                    this.ackflagclearflagtrue(str);\n                     this.multipleSelection = [];\n                    // this.$message.success('删除成功');\n                    // this.tableData.splice(index, 1);\n                })\n                .catch(() => {});\n        },\n        ackflagclearflagtrue(str){   \n            this.$axios({\n                method: \"POST\",\n                url: '/sys/warn/clearflag/true',\n                data: JSON.stringify(str),\n                // 设置请求头\n               headers: {\n                    'Content-Type': \"application/json\"\n                },\n                 withCredentials:true,//跨域请求\n            }).then((res) => {\n                let data = res.data\n                if(data.code==0) {         \n                    this.$message.success('操作完成') ;\n                    this.getData();//局部刷新表格               \n                } else {\n                    this.$message.error(data.msg || '操作失败')\n                }\n            }).catch((e)=>{\n                this.$message.error(e)\n            });\n        },\n\n        // 多选操作\n        handleSelectionChange(val) {\n            this.multipleSelection = val;\n        },\n        // 分页导航\n        // 每页显示的条数改变\n        changeSizeHandle(val) {\n            this.query.pageSize = val         // 改变每页显示的条数 \n            this.query.pageIndex = 1          // 注意：在改变每页显示的条数时，要将页码显示到第一页\n            this.getData()                    // 点击每页显示的条数时，显示第一页\n            console.log(`每页 ${val} 条`)\n        },\n        // current-change用于监听页数改变，而内容也发生改变\n        currentChangeHandle(val) {\n            this.query.pageIndex = val            // 改变默认的页数\n            this.getData()                        // 切换页码时，要获取每页显示的条数\n            console.log(`当前页: ${val}`)\n        }\n    },\n   mounted: function() {\n       this.getDevice();\n       this.getData();\n      \n   }\n\n};\n</script>\n\n<style >\n\n.letf-items{\nmargin-bottom: 10px;\n}\n\n.right-items{\nmargin-bottom: 10px;\n}\n  .el-header {\n    background-color: #B3C0D1;\n    color: #333;\n    text-align: center;\n    /* line-height: 60px; */\n    width: 1100px;\n    height: 90px;\n  }\n  \n\n  .box{\n  /* width: 1000px;\n     height: 400px; */\n     display: flex; ;\n     background-color: #E9EEF3;\n\t/* border:1px solid black; */\n    /* flex-direction: column; */\n  }\n  .el-main {\n      display: flex; \n      background-color: #E9EEF3;\n      color: #333;\n      text-align: center;\n      line-height: 140px;\n      width: 1100px;\n      height: 435px;\n    }\n  \n  body > .el-container {\n    margin-bottom: 20px;\n  }\n\n  .cont{\n      position: relative;\n      width:660px;\n      height: 250px;\n      border:1px solid black;\n      margin: 15px 2px;\n      display: flex;\n  }\n\n.contsolt{\n      position: relative;\n      width:45px; \n      height:200px;\n      background-color: #e7e7e7; \n      border:1px solid black;\n      margin: 10px 2px;\n}\n\n  /* 复选框 */\n.checkbox{\n    position: absolute;\n    bottom:-30px;\n    left: 10px;\n}\n  /* 状态 */\n.status{\n    position: absolute;\n    bottom:170px;\n    font-size: 12px;\n    left: 10px;\n    color:blue;\n}\n  /* 状态显示 */\n  .image {\n     position: absolute; \n     width: 20px;  \n     bottom:150px;\n     left: 10px;\n}\n  /* cpu数*/\n  .cpu{\n    position: absolute;\n    font-size: 12px;\n    bottom:130px;\n    left: 10px;\n  }\n  /* cpu数值 */\n  .bat{\n    position: absolute;\n    bottom:110px;\n    left: 10px;\n    width: 18px;\n    height: 18px;\n  }\n  /* 内存 */\n  .memory{\n    position: absolute;\n    font-size: 12px;\n    bottom:90px;\n    left: 10px;\n  }\n  /* 内存数值 */\n  .memorybat{\n    position: absolute;\n    bottom:70px;\n    left: 10px;\n  }\n /* SSD */\n  .ssd{\n    position: absolute;\n    font-size: 12px;\n    bottom:50px;\n    left: 10px;\n  }\n  /* 内存数值 */\n  .ssdbat{\n    position: absolute;\n    bottom:30px;\n    left: 10px;\n  }\n\n  /* 刀片名称*/\n  .name{\n    position: absolute;\n    font-size: 12px;\n    bottom:0px;\n    left: 5px;\n    transform: scale(0.75);\n    white-space: nowrap;  /*强制span不换行*/\n    display: inline-block;  /*将span当做块级元素对待*/\n    width: 25px;  /*限制宽度*/\n  }\n\n.center{\n    position: relative;\n    left: 80px;\n    \n}\n\n.center div{\n    position: relative;\n    display: inline-block;\n    margin: 5px 10px;\n}\n\n.moring{\n    position: absolute;\n    bottom:-30px;\n    left: 10px;\n}\n  \n</style>"]}]}