AAOA,QAAA,eAAA,EAAA;AAPA,OAAA,EAQA,IARA,CAQA,UAAA,GAAA,EAAA;AACA,YAAA,IAAA,GAAA,GAAA,CAAA,IAAA;;AACA,YAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA;AACA;AACA,UAAA,MAAA,CAAA,OAAA,GAHA,CAGA;;AACA,SAJA,MAIA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA,IAAA,MAAA;AACA;AACA,OAjBA,EAiBA,KAjBA,CAiBA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,CAAA;AACA,OAnBA;AAoBA,KAhNA;AAiNA;AACA;AACA,IAAA,gBAnNA,4BAmNA,GAnNA,EAmNA;AACA,WAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CADA,CACA;;AACA,WAAA,KAAA,CAAA,SAAA,GAAA,CAAA,CAFA,CAEA;;AACA,WAAA,OAAA,GAHA,CAGA;;AACA,MAAA,OAAA,CAAA,GAAA,wBAAA,GAAA;AACA,KAxNA;AA0NA;AACA,IAAA,mBA3NA,+BA2NA,GA3NA,EA2NA;AACA,WAAA,KAAA,CAAA,SAAA,GAAA,GAAA,CADA,CACA;;AACA,WAAA,OAAA,GAFA,CAEA;;AACA,MAAA,OAAA,CAAA,GAAA,+BAAA,GAAA;AACA;AA/NA;AAjDA,CAAA","sourcesContent":["<template>\r\n<div>\r\n    <div class=\"crumbs\">\r\n        <el-breadcrumb separator=\"/\">\r\n            <el-breadcrumb-item>\r\n                <i class=\"el-icon-lx-cascades\"></i> 角色管理\r\n            </el-breadcrumb-item>\r\n        </el-breadcrumb>\r\n    </div>\r\n    <div class=\"container\">\r\n        <div class=\"handle-box\">\r\n            <el-button class=\"handle-add mr10\" @click=\"addFile\"><i class=\"el-icon-circle-plus\" style=\"font-size:12px;color:#00EC00;margin-right: 5px;\"></i>增加</el-button>\r\n            <el-input placeholder=\"角色名\" v-model=\"query.roleName\" class=\"handle-input mr10\"></el-input>\r\n            <el-button @click=\"handleSearch\"><i class=\"el-icon-search\" style=\"font-size:12px;color:blue;margin-right: 5px;\"></i>搜索</el-button>\r\n        </div>\r\n        <el-table :data=\"tableData1\" border class=\"table\" ref=\"multipleTable\" header-cell-class-name=\"table-header\" @selection-change=\"handleSelectionChange\">\r\n            <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n            <el-table-column prop=\"roleName\" label=\"角色名称\"></el-table-column>\r\n            <el-table-column prop=\"remark\" label=\"备注\" align=\"center\">\r\n            </el-table-column>\r\n            <el-table-column prop=\"createTime\" label=\"上传时间\"></el-table-column>\r\n            <el-table-column prop=\"editTime\" label=\"修改时间\" align=\"center\">\r\n            </el-table-column>\r\n\r\n            <el-table-column label=\"操作\" width=\"180\" align=\"center\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button type=\"text\" icon=\"el-icon-edit\" @click=\"handleEdit(scope.$index, scope.row)\">编辑</el-button>\r\n                    <el-button type=\"text\" icon=\"el-icon-delete\" class=\"red\" @click=\"handleDelete(scope.$index, scope.row)\">删除</el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <div class=\"pagination\">\r\n            <el-pagination @size-change=\"changeSizeHandle\" @current-change=\"currentChangeHandle\" :current-page=\"query.pageIndex\" layout=\"total, sizes, prev, pager, next, jumper\" :total=\"pageTotal\" background>\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n\r\n    <!-- 编辑弹出框 -->\r\n    <el-dialog title=\"编辑用户\" :visible.sync=\"editVisible\" width=\"30%\">\r\n        <div style=\"height:400px;\" class=\"scrollbar\">\r\n            <el-scrollbar style=\"height:100%\">\r\n                <el-form ref=\"form\" :model=\"form\" label-width=\"100px\" style=\"width: 300px; margin-left:50px;\">\r\n                    <el-form-item label=\"角色名称\">\r\n                        <el-input v-model=\"form.roleName\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"角色编码\">\r\n                        <el-input v-model=\"form.remark\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"授权：\">\r\n                        <ul id=\"menuTree\" class=\"ztree\"></ul>\r\n                    </el-form-item>\r\n                </el-form>\r\n            </el-scrollbar>\r\n        </div>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"editVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"saveEdit\">确 定</el-button>\r\n        </span>\r\n    </el-dialog>\r\n\r\n    <!--用户增加弹出框-->\r\n    <el-dialog title=\"增加用户 \" :visible.sync=\"addVisible\" width=\"30%\">\r\n        <div style=\"height:400px;\" class=\"scrollbar\">\r\n            <el-scrollbar style=\"height:100%\">\r\n                <el-form ref=\"form\" :model=\"role\" label-width=\"100px\" style=\"width: 300px; margin-left:50px;\">\r\n                    <el-form-item label=\"角色名称\">\r\n                        <el-input v-model=\"role.roleName\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"角色编码\">\r\n                        <el-input v-model=\"role.remark\"></el-input>\r\n                    </el-form-item>\r\n\r\n                    <el-form-item label=\"授权：\">\r\n                        <ul id=\"menuTree\" class=\"ztree\"></ul>\r\n                    </el-form-item>\r\n\r\n                </el-form>\r\n            </el-scrollbar>\r\n        </div>\r\n        <span slot=\"footer\" class=\"dialog-footer\">\r\n            <el-button @click=\"addVisible = false\">取 消</el-button>\r\n            <el-button type=\"primary\" @click=\"saveAdd\">确 定</el-button>\r\n        </span>\r\n    </el-dialog>\r\n\r\n</div>\r\n</template>\r\n\r\n<script>\r\n// import { fetchData } from '../../api/index';\r\nexport default {\r\n    name: 'basetable',\r\n    data() {\r\n        return {\r\n            query: {\r\n                roleName: '',\r\n                limit: 10,\r\n                pageIndex: 1,\r\n                pageSize: 10\r\n            },\r\n            tableData1: [],\r\n            multipleSelection: [],\r\n            delList: [],\r\n            editVisible: false,\r\n            addVisible: false,\r\n            role: {\r\n                roleId: 0,\r\n                roleName: '',\r\n                remark: '',\r\n                menuIdList: []\r\n            },\r\n            pageTotal: 0,\r\n            form: {},\r\n            idx: -1,\r\n            id: -1,\r\n            ztree: '',\r\n            setting: {\r\n                data: {\r\n                    simpleData: {\r\n                        enable: true,\r\n                        idKey: \"id\",\r\n                        pIdKey: \"pId\",\r\n                        rootPId: -1\r\n                    },\r\n                    key: {\r\n                        url: \"nourl\"\r\n                    }\r\n                },\r\n                check: {\r\n                    enable: true,\r\n                    nocheckInherit: true\r\n                }\r\n            }\r\n        };\r\n    },\r\n    created() {\r\n        this.getData();\r\n\r\n    },\r\n    methods: {\r\n        checkTreeData() {\r\n            this.$axios({\r\n                method: \"post\",\r\n                url: '/sys/role/info/' + this.form.roleId,\r\n                headers: {\r\n                    'Content-Type': \"application/json\"\r\n                },\r\n                withCredentials: true,\r\n            }).then((res) => {\r\n                if (res.data.code === 0) {\r\n                    //勾选角色所拥有的菜单\r\n                    var menuIds = res.data.role.menuIdList;\r\n                    for (var i = 0; i < menuIds.length; i++) {\r\n                        var node = this.ztree.getNodeByParam(\"id\", menuIds[i]);\r\n                        if (node != null) {\r\n                            this.ztree.checkNode(node, true, false);\r\n                        }\r\n                    }\r\n                }\r\n            }).catch((e) => {\r\n                this.$message.error(e)\r\n            });\r\n        },\r\n        getTreeData() {\r\n            this.$axios({\r\n                method: \"get\",\r\n                url: 'sys/menu/treePerms',\r\n                // headers: {\r\n                //     'Content-Type': \"application/json\"\r\n                //  },\r\n                withCredentials: true,\r\n            }).then((res) => {\r\n                let data = res.data;\r\n                if (data.code == 0) {\r\n                    this.ztree = $.fn.zTree.init($(\"#menuTree\"), this.setting, data.data);\r\n                    //展开所有节点\r\n                    this.ztree.expandAll(true);\r\n                } else {\r\n                    this.$message.error(data.msg || '操作失败')\r\n                }\r\n            }).catch((e) => {\r\n                this.$message.error(e)\r\n            });\r\n        },\r\n        // 获取 easy-mock 的模拟数据\r\n        getData() {\r\n            this.$axios({\r\n                method: \"post\",\r\n                url: 'sys/role/listData',\r\n                data: JSON.stringify({\r\n                    page: this.query.pageIndex,\r\n                    limit: this.query.pageSize,\r\n                    roleName: this.query.roleName\r\n                }),\r\n                headers: {\r\n                    'Content-Type': \"application/json\"\r\n                },\r\n                withCredentials: true,\r\n            }).then(res => {\r\n                if (res.data.code == 0) {\r\n                    this.tableData1 = res.data.page.list;\r\n                    this.pageTotal = res.data.page.total || 10; //1页数据\r\n                    this.query.pageIndex = res.data.page.page; //当前页\r\n                    this.query.pageSize = res.data.page.limit; //数据总数\r\n                } else {\r\n                    this.$message.error(res.data.msg);\r\n                }\r\n            }).catch(error => {\r\n                this.$message.error('获取数据失败！');\r\n            });\r\n\r\n        },\r\n        // 触发搜索按钮\r\n        handleSearch() {\r\n            this.getData();\r\n        },\r\n        // 删除操作\r\n        handleDelete(index, row) {\r\n            this.id = row.userId;\r\n            console.log(this.id);\r\n            // 二次确认删除\r\n            this.$confirm('确定要删除吗？', '提示', {\r\n                type: 'warning'\r\n            }).then(() => {\r\n                this.saveDelete();\r\n            }).catch(() => {});\r\n        },\r\n        saveDelete() {\r\n            let ids = [];\r\n            ids.push(this.id + \"\");\r\n            console.log(ids);\r\n            this.$axios({\r\n                method: \"post\",\r\n                url: '/sys/user/delete',\r\n                data: JSON.stringify(ids),\r\n                // 设置请求头\r\n                headers: {\r\n                    'Content-Type': \"application/json\"\r\n                },\r\n                withCredentials: true, //跨域请求\r\n            }).then((res) => {\r\n                let data = res.data\r\n                if (data.code == 0) {\r\n                    this.$message.success('操作完成');\r\n                    this.getData(); //局部刷新表格               \r\n                } else {\r\n                    this.$message.error(data.msg || '操作失败')\r\n                }\r\n            }).catch((e) => {\r\n                this.$message.error(e)\r\n            });\r\n        },\r\n        // 多选操作\r\n        handleSelectionChange(val) {\r\n            this.multipleSelection = val;\r\n        },\r\n        delAllSelection() {\r\n            const length = this.multipleSelection.length;\r\n            let str = [];\r\n            this.delList = this.delList.concat(this.multipleSelection);\r\n\r\n            for (let i = 0; i < this.delList.length; i++) {\r\n                str.push(this.delList[i].id + \"\");\r\n            }\r\n            console.log(str);\r\n            this.saveDelete(str);\r\n            this.multipleSelection = [];\r\n        },\r\n        addFile() {\r\n            this.addVisible = true;\r\n            this.getTreeData();\r\n        },\r\n        saveAdd() {\r\n            this.addVisible = false;\r\n\r\n            //获取选择的菜单\r\n            var nodes = this.ztree.getCheckedNodes(true);\r\n            var menuIdList = new Array();\r\n            for (var i = 0; i < nodes.length; i++) {\r\n                menuIdList.push(nodes[i].id);\r\n            }\r\n            //向form表单中添加选中的菜单\r\n            this.role.menuIdList = menuIdList;\r\n\r\n            this.$axios({\r\n                method: \"POST\",\r\n                url: '/sys/role/save',\r\n                data: JSON.stringify(this.role),\r\n                headers: {\r\n                    'Content-Type': \"application/json\"\r\n                },\r\n                withCredentials: true,\r\n            }).then((res) => {\r\n                let data = res.data\r\n                if (data.code == 0) {\r\n                    this.$message.success('操作完成');\r\n                    this.getData(); //局部刷新表格               \r\n                } else {\r\n                    this.$message.error(data.msg || '操作失败')\r\n                }\r\n                this.uploadLoading = false\r\n            }).catch((e) => {\r\n                this.uploadLoading = false\r\n                this.$message.error(e)\r\n            });\r\n        },\r\n        // 编辑操作\r\n        handleEdit(index, row) {\r\n            this.form = row;\r\n\r\n            this.editVisible = true;\r\n            this.getTreeData();\r\n            this.checkTreeData();\r\n        },\r\n        // 保存编辑\r\n        saveEdit() {\r\n            this.editVisible = false;\r\n\r\n            //获取选择的菜单\r\n            var nodes = this.ztree.getCheckedNodes(true);\r\n            var menuIdList = new Array();\r\n            for (var i = 0; i < nodes.length; i++) {\r\n                menuIdList.push(nodes[i].id);\r\n            }\r\n            //向form表单中添加选中的菜单\r\n            this.form.menuIdList = menuIdList;\r\n            // 文件上传\r\n            this.$axios({\r\n                method: \"POST\",\r\n                url: '/sys/role/update',\r\n                data: JSON.stringify(this.form),\r\n                headers: {\r\n                    'Content-Type': \"application/json\"\r\n                },\r\n                withCredentials: true,\r\n            }).then((res) => {\r\n                let data = res.data\r\n                if (data.code == 0) {\r\n                    // this.$message.success('操作完成') ;\r\n                    // this.$message.success(`修改第 ${this.idx + 1} 行成功`);\r\n                    this.getData(); //局部刷新表格               \r\n                } else {\r\n                    this.$message.error(data.msg || '操作失败')\r\n                }\r\n            }).catch((e) => {\r\n                this.$message.error(e)\r\n            });\r\n        },\r\n        // 分页导航\r\n        // 每页显示的条数改变\r\n        changeSizeHandle(val) {\r\n            this.query.pageSize = val // 改变每页显示的条数 \r\n            this.query.pageIndex = 1 // 注意：在改变每页显示的条数时，要将页码显示到第一页\r\n            this.getData() // 点击每页显示的条数时，显示第一页\r\n            console.log(`每页 ${val} 条`)\r\n        },\r\n\r\n        // current-change用于监听页数改变，而内容也发生改变\r\n        currentChangeHandle(val) {\r\n            this.query.pageIndex = val // 改变默认的页数\r\n            this.getData() // 切换页码时，要获取每页显示的条数\r\n            console.log(`当前页: ${val}`)\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.handle-box {\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.handle-select {\r\n    width: 120px;\r\n}\r\n\r\n.handle-input {\r\n    width: 200px;\r\n    display: inline-block;\r\n}\r\n\r\n.table {\r\n    width: 100%;\r\n    font-size: 14px;\r\n}\r\n\r\n.red {\r\n    color: #ff0000;\r\n}\r\n\r\n.mr10 {\r\n    margin-right: 10px;\r\n}\r\n\r\n.table-td-thumb {\r\n    display: block;\r\n    margin: auto;\r\n    width: 40px;\r\n    height: 40px;\r\n}\r\n\r\n/deep/ .scrollbar {\r\n    white-space: nowrap;\r\n\r\n    /*强制一行显示（看需要）*/\r\n    .el-scrollbar {\r\n        display: flex;\r\n        justify-content: space-around;\r\n        padding: 0 0px;\r\n    }\r\n\r\n    /deep/ .el-scrollbar__wrap {\r\n        /* 必要的*/\r\n        overflow: scroll;\r\n        width: 110%;\r\n        /*隐藏侧边默认滚动条 （达不到效果可以自己试着微调） 如解决不了可尝试用 `margin-right -60px`*/\r\n        height: 110%;\r\n        /*隐藏底部滚动条  （100%如果达不到效果可以试着用110%）   */\r\n    }\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/Role"}]}