ut> -->\r\n                    <input type=\"file\"  id=\"people-export\" ref=\"inputer\" />\r\n                </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"editVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"saveEdit\">确 定</el-button>\r\n            </span>\r\n        </el-dialog>\r\n\r\n        <!--文件上传弹出框-->\r\n          <el-dialog title=\"文件上传 \" :visible.sync=\"addVisible\" width=\"30%\">\r\n            <el-form ref=\"form\" :model=\"file\" label-width=\"70px\" style=\"width: 300px; margin-left:50px;\">\r\n                <el-form-item label=\"文件名\">\r\n                    <el-input v-model=\"file.fileName\" ></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"文件 \">\r\n                    <!-- <el-input v-model=\"form.address\"></el-input> -->\r\n                    <input type=\"file\"  ref=\"inputer\"/>\r\n                </el-form-item>\r\n            </el-form>\r\n            <span slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"addVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"saveAdd\">确 定</el-button>\r\n            </span>\r\n        </el-dialog>\r\n\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n// import { fetchData } from '../../api/index';\r\nexport default {\r\n    name: 'basetable',\r\n    data() {\r\n        return {\r\n            query: {\r\n                // url: '',\r\n                fileName: '',\r\n                limit:10,\r\n                pageIndex: 1,\r\n                pageSize: 10\r\n            },\r\n            tableData1: [],\r\n            multipleSelection: [],\r\n            delList: [],\r\n            editVisible: false,\r\n            addVisible:false,\r\n            file:{\r\n                fileName: '',\r\n                url:'',\r\n            },\r\n            pageTotal: 0,\r\n            form: {},\r\n            idx: -1,\r\n            id: -1,\r\n        };\r\n    },\r\n    created() {\r\n        this.getData();\r\n    },\r\n    methods: {\r\n\r\n        // stateFormat(row, column) {\r\n        //     console.log(row.grade)\r\n        //     if (row.grade === 0) {\r\n        //         return '重要'\r\n        //     } else if (row.grade === 1) {\r\n        //          return '紧急'\r\n        //     } else if (row.grade === 2) {\r\n        //         return '提示'\r\n        //     }  else if(row.grade === 3){\r\n        //         return '次要'\r\n        //     }\r\n        // },\r\n        // 获取 easy-mock 的模拟数据\r\n        getData(str) {\r\n            // fetchData(this.query).then(res => {\r\n            //     console.log(res);\r\n            //     this.tableData1 = res.list;\r\n            //     this.pageTotal = res.pageTotal || 50;\r\n            // });\r\n            if(str!=undefined){\r\n                this.query.pageIndex=str;\r\n            }\r\n            console.log(this.query);\r\n                this.$axios({\r\n                    methods:\"post\",\r\n                    url:'/sys/warn/listData',\r\n                    data:JSON.stringify(this.query),\r\n                    headers: {\r\n                    'Content-Type': 'text/plain'\r\n                   },\r\n                    withCredentials:true,\r\n                }).then(res => {\r\n                    if(res.data.code==0){          \r\n                        // for (let index = 0; index < res.data.page.list.length; index++) {\r\n                        //     const element = res.data.page.list[index];\r\n                        //     element.fileSize=(element.fileSize/1024).toFixed(2);\r\n                        // }                                               \r\n                        this.tableData1 = res.data.page.list;\r\n                        console.log(res.data.page.list);\r\n                        this.pageTotal = res.data.page.totalCount ||