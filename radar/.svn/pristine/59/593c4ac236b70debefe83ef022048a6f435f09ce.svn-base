tyle=\"{padding: '0px'}\" style=\"height:450px;width:1750px;\">\r\n                        <div slot=\"header\" class=\"clearfix\">\r\n                            <span>CPU使用情况</span>\r\n                       </div>    \r\n                            <div id=\"myChart\" :style=\"{width: '1700px', height: '350px'}\"></div>\r\n                        </el-card>\r\n                    </el-col>\r\n                </el-row>\r\n                <el-row :gutter=\"20\" class=\"mgb20\">\r\n                    <el-col :span=\"12\">\r\n                        <el-card shadow=\"hover\" :body-style=\"{padding: '0px'}\" style=\"height: 500px;width:100%\">\r\n                            <div slot=\"header\" class=\"clearfix\">\r\n                                <h3>内存占用率</h3>\r\n                        </div>    \r\n                        <div id=\"container\" :style=\"{width: '100%', height: '500px'}\"></div>\r\n                        </el-card>\r\n                    </el-col>\r\n                    <el-col :span=\"12\">\r\n                        <el-card shadow=\"hover\" :body-style=\"{padding: '0px'}\" style=\"height: 500px;width:1150px;\">\r\n                        <div slot=\"header\" class=\"clearfix\">\r\n                            <h3>磁盘占用率</h3>\r\n                       </div>    \r\n                        <div id=\"container2\" :style=\"{width: '100%', height: '500px'}\"></div>\r\n                        </el-card>\r\n                    </el-col>\r\n                </el-row>\r\n            </el-col> \r\n    </el-tab-pane>\r\n    <el-tab-pane label=\"状态\" name=\"second\" >\r\n\r\n    </el-tab-pane>\r\n  </el-tabs>\r\n</div>  \r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data(){\r\n        return{\r\n            activeName: 'first',\r\n            input:'10',\r\n            checked:false,\r\n            cpu:'',\r\n            ssd:'',\r\n            mem:'',\r\n            drawer: false,\r\n        }\r\n    },\r\n    methods:{\r\n        drawC(){          \r\n            let myChart = this.$echarts.init(document.getElementById('container'));\r\n            myChart.setOption({\r\n              title: {\r\n                // text: '某站点用户访问来源',\r\n                // subtext: '纯属虚构',\r\n                // left: 'center'\r\n                },\r\n                tooltip: {\r\n                    trigger: 'item',\r\n                    formatter: '{a} <br/>{b} : {c} ({d}%)'\r\n                },\r\n                legend: {\r\n                    orient: 'vertical',\r\n                    left: 'left',\r\n                    data: ['已占用内存', '未占用内存']\r\n                },\r\n                series: [\r\n                    {\r\n                    name: '访问来源',\r\n                    type: 'pie',\r\n                    radius: '55%',\r\n                    center: ['50%', '50%'],\r\n                    data: [\r\n                    ],\r\n                    emphasis: {\r\n                        itemStyle: {\r\n                            shadowBlur: 10,\r\n                            shadowOffsetX: 0,\r\n                            shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n                        }\r\n                    }\r\n                    }\r\n                ]\r\n            });\r\n        },\r\n         drawC2(){\r\n            let myChart = this.$echarts.init(document.getElementById('container2'));\r\n            myChart.setOption({\r\n                    title: {\r\n                // text: '某站点用户访问来源',\r\n                // subtext: '纯属虚构',\r\n                // left: 'center'\r\n                },\r\n                tooltip: {\r\n                    trigger: 'item',\r\n                    formatter: '{a} <br/>{b} : {c} ({d}%)'\r\n                },\r\n                legend: {\r\n                    orient: 'vertical',\r\n                    left: 'left',\r\n                    data: ['已占用磁盘', '未占用磁盘']\r\n                },\r\n                series: [\r\n                    {\r\n                    name: '访问来源',\r\n                    type: 'pie',\r\n                    radius: '55%',\r\n                    center: ['35%', '50%'],\r\n                    data: [\r\n                        // {value: 310, name: '未占用磁盘'},\r\n                    ],\r\n                    emphasis: {\r\n                        itemStyle: {\r\n                            shadowBlur: 10,\r\n                            shadowOffsetX: 0,\r\n                            shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n                        }\r\n                    }\r\n                    }\r\n                ]\r\n            });\r\n        },\r\n        drawLine(){\r\n        // 基于准备好的dom，初始化echarts实例\r\n        let myChart = this.$echarts.init(document.getElementById('myChart'));\r\n        myChart.setOption({\r\n            title : {\r\n                text: 'CPU',\r\n                subtext: '',\r\n                left: 'center'\r\n            },\r\n            tooltip : {\r\n                trigger: 'axis',\r\n                axisPointer: {\r\n                 type: 'cross'\r\n             }\r\n            },\r\n            legend: {\r\n                data:['意向','预购','成交'],\r\n                bottom: 0\r\n            },\r\n            toolbox: {\r\n                show : true,\r\n                feature : {\r\n                    mark : {show: true},\r\n                    dataView : {show: true, readOnly: false},\r\n                    magicType : {show: true, type: ['line', 'bar', 'stack', 'tiled']},\r\n                    restore : {show: true},\r\n                    saveAsImage : {show: true}\r\n                }\r\n            },\r\n            calculable : true,\r\n            xAxis : [\r\n                {\r\n                    type : 'category',\r\n                    boundaryGap : false,\r\n                    data : []\r\n                }\r\n            ],\r\n            yAxis : [\r\n                {\r\n                    type : 'value',\r\n                    boundaryGap : false,\r\n                    max:100\r\n                }\r\n            ],\r\n            series : []\r\n        });\r\n      },\r\n      getCPURate(){\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/history/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){                                                     \r\n                        let myChart = this.$echarts.init(document.getElementById('myChart'));\r\n                        myChart.setOption({\r\n                              xAxis: {\r\n                                  data: res.data.data.status.times\r\n                              },\r\n                              series: [{\r\n                                  type:\"line\",\r\n                                  data:res.data.data.status.datas.memRate\r\n                              }]\r\n                          });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                });\r\n      },\r\n        getSSDRate(){\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){                                                     \r\n                        let myChart = this.$echarts.init(document.getElementById('container2'));\r\n                        myChart.setOption({\r\n                              series: [{\r\n                                  data:[{\r\n                                      value:res.data.data.status.SSDRate,\r\n                                      name: '已占用磁盘'\r\n                                  },\r\n                                  {\r\n                                      value:100-res.data.data.status.SSDRate,\r\n                                      name: '未占用磁盘'\r\n                                  }\r\n                                  ]\r\n                              }]\r\n                          });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                });\r\n      },\r\n        getMemRate(){\r\n            this.$axios({\r\n                    method:\"get\",\r\n                    url:'sys/device/status/id?deviceId='+this.$route.params.menuId,\r\n                    withCredentials:true,\r\n                }).then(res => {                   \r\n                    if(res.data.code==0){                                                     \r\n                        let myChart = this.$echarts.init(document.getElementById('container'));\r\n                        myChart.setOption({\r\n                              series: [{\r\n                                  data:[{\r\n                                      value:res.data.data.status.memRate,\r\n                                      name: '已占用内存'\r\n                                  },\r\n                                  {\r\n                                      value:100-res.data.data.status.memRate,\r\n                                      name: '未占用内存'\r\n                                  }\r\n                                  ]\r\n                              }]\r\n                          });\r\n                    }else{\r\n                         this.$message.error(res.data.msg);\r\n                    }\r\n                }).catch(error => {\r\n                    this.$message.error('获取数据失败！');\r\n                });\r\n      },\r\n      checkedFun(e,checkedFresh){\r\n            if(e==true){\r\n                 this.cpu=setInterval(this.getCPURate,(parseInt(this.input) *1000));\r\n                 this.ssd=setInterval(this.getSSDRate,(parseInt(this.input) *1000));\r\n                 this.mem=setInterval(this.getMemRate,(parseInt(this.input) *1000));\r\n            }else{\r\n                clearInterval(this.cpu);\r\n                clearInterval(this.ssd);\r\n                clearInterval(this.mem);\r\n            }\r\n      }\r\n\r\n    },\r\n    mounted:function(){\r\n        this.drawC();\r\n        this.drawC2();\r\n        this.drawLine();\r\n        this.getCPURate();\r\n        this.getSSDRate();\r\n        this.getMemRate();\r\n    }\r\n}\r\n</script>\r\n<style>\r\n.Anchor_fixmain{\r\n     position: fixed;\r\n     top: 0;\r\n     left: 0;\r\n     z-index: 101;\r\n     border-left: 1px #ccc solid;\r\n     width: 0;\r\n     height: 100%;\r\n     -webkit-box-sizing: border-box;\r\n     -moz-box-sizing: border-box;\r\n     box-sizing: border-box;\r\n     background:#fff;\r\n}\r\n.btnlr_box{\r\n  position: absolute;\r\n  top: 50%;\r\n  right: -590px;\r\n  z-index: 101;\r\n  margin-top: -40px;\r\n  background-color: rgba(0,0,0,0.5);\r\n  -webkit-border-radius: 0 3px 3px 0;\r\n  -moz-border-radius:  0 3px 3px 0;\r\n  border-radius:  0 3px 3px 0;\r\n  width: 15px;\r\n  height: 80px;\r\n  font-size: 12px;\r\n  color: #FFFFFF;\r\n  line-height: 80px;\r\n}\r\n</style>"]}]}